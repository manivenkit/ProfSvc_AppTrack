<div class="alert alert-secondary mt-4" role="alert">
	<span aria-hidden="true" class="oi oi-pencil mr-2"></span>
	<strong>@Title</strong>

	<span class="text-nowrap">
		Please take our
		<a class="font-weight-bold" href="https://go.microsoft.com/fwlink/?linkid=2137813" target="_blank">brief survey</a>
	</span>
	and tell us what you think.
</div>

@code {
	// Demonstrates how a parent component can supply parameters
	[Parameter]
	public string Title
	{
		get;
		set;
	}

}